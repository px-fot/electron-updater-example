{
  "name": "electron-updater-example",
  "version": "0.2.6",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "dist-win" : "build --win",
    "dist-mac" : "build --mac"
  },
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-builder": "^12.3.1",
    "to": "^0.2.9",
    "update": "^0.7.4"
  },
  "dependencies": {
    "electron-log": "^1.3.0",
    "electron-updater": "^1.4.2"
  },
  "build": {
    "appId": "com.github.px-fot.electronupdaterexample",
    "publish": [
      {
        "provider": "github",
        "owner": "px-fot",
        "repo": "electron-updater-example"
      }
    ],
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    }
  }
}
